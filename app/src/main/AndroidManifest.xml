<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="choi.ccb.com.doublescroll">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PullUpToLoadMoreActivity" />
        <activity android:name=".EndScrollActivity" />
        <activity android:name=".SlideActivity" />
        <activity android:name=".Slide2Activity" />
        <activity android:name=".MyScrollActivity"></activity>
    </application>

</manifest>